@model Catalog.Core.DataService

<div>
    <h4>@Model.Teams.First().Name</h4>
</div>

<table class="table">

    <tbody>
        @foreach (var item in Model.People) {
            <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Name)
                    </td>
            </tr>
        }
    </tbody>
</table>
